  Vendas.Estilo;

{$R *.dfm}

procedure TForm1.FormCreate(Sender: TObject);
begin
 pnlTop.Color :=  colorBAckgroundTop;
 pnlRodape.Color :=  colorBAckgroundTop;
 pnlListagem.Color := colorBAckgroundTopSubmenu;
end;

end.
